#!/bin/bash


outfile=histo.ps




psbasemap -JX6i -R-2/2/0/1 -B0.25/0.1 -P -K > $outfile

awk '{if ($2==0.1) print $3,$4}' A10_PDF.out | \
psxy -J -R -O -K -Sc0.1c -Gblack >> $outfile





# Convert and open
psxy /dev/null -J -R -O >> $outfile
psconvert -Tg -A $outfile
echo "created file histo.png"
open histo.png
